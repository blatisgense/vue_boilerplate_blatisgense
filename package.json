{
  "name": "vue_boilerplate_blatisgense",
  "version": "3.4.21",
  "description": "Vue 3.4.21 boilerplate by @blatisgense",
  "type": "module",
  "types": "index.d.ts",
  "author": "Blatisgense",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blatisgense/vue_boilerplate_blatisgense.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --host",
    "sharp-images": "npx tsx ./src/helpers/sharp.ts",
    "format": "prettier --write ./src/**/*.ts tests/**/*.ts",
    "lint-fix": "eslint --fix ./src/**/*.{js,ts,json,vue} tests/**/*.ts",
    "cypress:open": "cypress open",
    "cypress:e2e": "cypress run --e2e",
    "vitest": "vitest --ui --coverage --outputFile tests/results/preview/preview"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/language-server": "^2.0.13",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.7.3",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "glob": "^10.3.12",
    "jsdom": "^24.0.0",
    "msw": "^2.2.13",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "sharp": "^0.33.3",
    "svgo": "^3.2.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.13"
  }
}
